{UTF-8}
Profit=
	TotalBenefit - TotalProjectCost
	~	円
	~		|

ProjectDelay=
	ProjectCompletionTime-ProjectCompletionTimePlanned
	~	月
	~		|

ProjectCompletionTimePlanned=
	25
	~	月
	~		|

EvaluatedReward=
	IF THEN ELSE(ProjectDelay <= 2, Profit / RewardByProfit , Profit / RewardByProfit)
	~	円*円/RewardUnit
	~		|

Benefit=
	Capability * BenefitByCapability
	~	円/月
	~		|

BenefitByCapability=
	100000
	~	円/CapabilityUnit
	~		|

TotalBenefit= INTEG (
	Benefit,
		0)
	~	円
	~		|

RewardByProfit=
	10000
	~	RewardUnit/円
	~		|

ProjectStartTime=
	13
	~	月
	~		|

CapabilityImprovement=
	IF THEN ELSE(ProjectCompletionTime = Time, ObtainedCapabiltyByProject , 0 )
	~	CapabilityUnit/月
	~		|

TotalProjectCost=
	2.5e+08
	~	円
	~		|

ObtainedCapabilityPlanned=
	100
	~	CapabilityUnit/月
	~		|

ProjectCompletionTime=
	25
	~	月
	~		|

ScopeChangeRate=
	1
	~	Dmnl
	~		|

ObtainedCapabiltyByProject=
	ObtainedCapabilityPlanned * ScopeChangeRate
	~	CapabilityUnit/月
	~		|

Capability= INTEG (
	CapabilityImprovement,
		InitialCapability)
	~	CapabilityUnit
	~		|

InitialCapability=
	0
	~	CapabilityUnit
	~		|

